{
  "name": "issues-client",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.tsx",
  "scripts": {
    "build": "esbuild src/index.tsx --bundle --outfile=build/bundle.js",
    "build-prod": "esbuild src/index.tsx --bundle --minify --outfile=build/bundle.js",
    "reinstall": "rm -rf node_modules && npm install",
    "ci-check": "npm run lint && npm run tsc && npm run test",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc -p ./ --noEmit",
    "tsc:watch": "tsc -p ./ --noEmit -w"
  },
  "dependencies": {
    "@types/jest": "27.5.0",
    "@types/node": "16.11.33",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "axios": "0.26.1",
    "bson": "^4.6.5",
    "ramda": "^0.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "7.2.8",
    "react-router-dom": "6.3.0",
    "react-scripts": "^2.1.3",
    "redux": "4.1.2",
    "redux-observable": "1.2.0",
    "redux-thunk": "2.4.1",
    "reselect": "4.0.0",
    "rxjs": "6.5.3",
